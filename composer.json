{
    "name": "symlabs/wamp",
    "type": "library",
    "description": "Router implementation of WAMP protocol",
    "keywords": [
        "websocket",
        "wamp",
        "wamp2"
    ],
    "homepage": "https://github.com/SymLabs/wamp",
    "license": "MIT",
    "authors": [
        {
            "name": "Saeed Asalisaf",
            "email": "asalisaf.saeed@gmail.com",
            "homepage": "https://github.com/asalisaf",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "cboden/ratchet": "^0.4.1",
        "symfony/messenger": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Symlabs\\WAMP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymLabs\\WAMP\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
